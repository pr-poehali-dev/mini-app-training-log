{
  "tests": [
    {
      "name": "Test get workouts without VK user ID",
      "method": "GET",
      "path": "/",
      "expectedStatus": 401,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Test get workouts with VK user ID",
      "method": "GET",
      "path": "/",
      "headers": {
        "x-vk-user-id": "123456789"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "workouts": []
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Test create workout",
      "method": "POST",
      "path": "/",
      "headers": {
        "x-vk-user-id": "123456789",
        "Content-Type": "application/json"
      },
      "body": {
        "name": "Test Workout",
        "date": "2024-01-01",
        "exercises": [
          {
            "name": "Push-ups",
            "sets": 3,
            "reps": 10,
            "weight": 0,
            "notes": "Test exercise"
          }
        ]
      },
      "expectedStatus": 200,
      "expectedBody": {
        "success": true,
        "workout_id": "number"
      },
      "bodyMatcher": "partial"
    }
  ]
}